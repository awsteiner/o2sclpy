o2graph -backend Agg -create tensor_grid 3 100 100 100 \
	-set-grid 0 func:100:i/100 \
	-set-grid 1 func:100:i/100 \
	-set-grid 2 func:100:i/100 \
	-function "sqrt(x0*x0+x1*x1+x2*x2)" -set yt_path yaw \
	-yt-tf new 0 1 -yt-tf gauss 0.5 0.05 0.8 0.0 0.0 0.5 \
	-yt-add-vol -xtitle "$ x $" -ytitle "$ y $" \
	-yt-render ytg_*.png ytg.mp4
