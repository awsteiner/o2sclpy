o2graph -backend Agg  -create tensor_grid 3 2 2 2 \
	-set-grid 0 func:2:i \
	-set-grid 1 func:2:i \
	-set-grid 2 func:2:i \
	-function "0" \
	-yt-tf new 0 1 -yt-tf gauss 2.0 0.05 0.8 0.0 0.0 0.5 \
	-yt-add-vol \
	-create table x "grid:0,5,0.01" \
	-function "sin(4*x)" y -function "cos(4*x)" z \
	-yt-scatter x y z -yt-render ys.png